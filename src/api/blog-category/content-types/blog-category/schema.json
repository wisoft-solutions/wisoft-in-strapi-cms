{
  "kind": "collectionType",
  "collectionName": "blog_categories",
  "info": {
    "singularName": "blog-category",
    "pluralName": "blog-categories",
    "displayName": "BlogCategory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Description": {
      "type": "text"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog.blog",
      "inversedBy": "Category"
    }
  }
}
